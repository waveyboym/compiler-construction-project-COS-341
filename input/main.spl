num F_addition(V_x, V_y, V_z)
{
    num V_result1, num V_result2, num V_result3,
    begin
        if eq(mul(V_z, 1), 3)
        then 
            begin
                V_result1 = add(V_x, add(V_y, V_z));
            end
        else
            begin
                V_result1 = mul(V_x, add(V_y, V_z));
            end;
        return V_result1;
    end
}
